#include <iostream>
#include <conio.h>

using namespace std;

int main()

{
	int n=20, a, b=0, c=1;
	
	cout << "20 Fibonacci Numbers: " << endl;
	
	for (int d=0; d<=n; d++)
	{
	if (d==0)
		{
		cout << b << "," << c << ",";
		}	
	else 
		{
		a = b+c;
		b = c;
		c = a;
		cout << a << ",";
		}
	}
	
	_getch();
	return 0;
}
