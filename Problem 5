#include <iostream>
#include <conio.h>

using namespace std;

int main()

{
	int count = 1;

	cout << "Counting... " << endl;
	
	while (count<10)
		{
		cout << count <<",";
		count = count + 1; 
		}	
	while (count>=10 && count<=30)
		{
		cout << count << ",";
		count = count + 2;
		}
	
	
	
	system ("PAUSE");
	_getch();
	return 0;
}
